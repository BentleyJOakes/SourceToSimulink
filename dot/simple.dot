"5 - Operator\n=" [style=filled, fillcolor=lightblue];
"6 - Switch" [style=filled, fillcolor=lightblue];
"0 - Gain\nint a" -> "3 - Operator\n>";
"2 - Constant\n50" -> "3 - Operator\n>";
"1 - Constant\n100" -> "5 - Operator\n=";
"4 - Constant\n10" -> "5 - Operator\n=";
"3 - Operator\n>" -> "6 - Switch";
"5 - Operator\n=" -> "6 - Switch";
